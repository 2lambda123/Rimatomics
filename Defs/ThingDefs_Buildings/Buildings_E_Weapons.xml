    <specialDisplayRadius>55</specialDisplayRadius>
    <statBases>
      <MaxHitPoints>480</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <WorkToBuild>2700</WorkToBuild>
      <Mass>200</Mass>
      <Beauty>-60</Beauty>
    </statBases>
    <building>
      <turretShellDef>RailgunSabot</turretShellDef>
      <turretBurstwarmupTime>5.0</turretBurstwarmupTime>
      <turretBurstCooldownTime>35.0</turretBurstCooldownTime>
      <buildingTags>
        <li>Artillery</li>
      </buildingTags>
      <turretGunDef>Artillery_PPCRailgun</turretGunDef>
      <turretTopgraphicPath>Things/RimatomicsBuildings/Railgun_Top</turretTopgraphicPath>
    </building>
    <placeWorkers>
      <li>PlaceWorker_NotUnderRoof</li>
      <li>Rimatomics.PlaceWorker_ResearchStage</li>
    </placeWorkers>
    <costList>
      <Steel>650</Steel>
      <Component>19</Component>
    </costList>
    <designationHotKey>Misc3</designationHotKey>
    <uiIconPath>UI/Railgun</uiIconPath>
    <rimatomicsPrerequisites>
      <li>Railgun3</li>
    </rimatomicsPrerequisites>
  </ThingDef>

  <ThingDef>
    <defName>Artillery_PPCRailgun</defName>
    <label>explosive mortar</label>
    <description>Mortar launcher. Lobs bombs over walls. Very inaccurate but long-ranged.</description>
    <category>Item</category>
    <thingClass>ThingWithComps</thingClass>
    <equipmentType>Primary</equipmentType>
    <tickerType>Never</tickerType>
    <techLevel>Industrial</techLevel>
    <destroyOnDrop>True</destroyOnDrop>
    <menuHidden>True</menuHidden>
    <tradeability>Never</tradeability>
    <weaponTags>
      <li>Artillery</li>
    </weaponTags>
    <comps>
      <li>
        <compClass>CompEquippable</compClass>
      </li>
    </comps>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <forceNormalTimeSpeed>false</forceNormalTimeSpeed>
        <hasStandardCommand>true</hasStandardCommand>
        <projectileDef>Bullet_PPCRailgun</projectileDef>
        <forcedMissRadius>6</forcedMissRadius>
        <warmupTime>5.8</warmupTime>
        <minRange>30</minRange>
        <range>500</range>
        <burstShotCount>1</burstShotCount>
        <soundCast>Mortar_LaunchA</soundCast>
        <muzzleFlashScale>24</muzzleFlashScale>
      </li>
    </verbs>
  </ThingDef>

  <ThingDef Name="BaseSabot" Abstract="True">
    <category>Projectile</category>
    <tickerType>Normal</tickerType>
    <altitudeLayer>Projectile</altitudeLayer>
    <thingClass>Bullet</thingClass>
    <label>bullet</label>
    <useHitPoints>False</useHitPoints>
    <neverMultiSelect>True</neverMultiSelect>
    <graphicData>
      <shaderType>Transparent</shaderType>
    </graphicData>
  </ThingDef>

  <ThingDef ParentName="BaseSabot">
    <defName>Bullet_PPCRailgun</defName>
    <label>explosive mortar shell</label>
    <graphicData>
      <texPath>Things/Resources/SabotRound</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>TransparentPostLight</shaderType>
    </graphicData>
    <thingClass>Projectile_Explosive</thingClass>
    <projectile>
      <damageDef>Bomb</damageDef>
      <damageAmountBase>60</damageAmountBase>
      <speed>150</speed>
      <explosionRadius>6.5</explosionRadius>
      <flyOverhead>true</flyOverhead>
      <soundHitThickRoof>ArtilleryHitThickRoof</soundHitThickRoof>
      <soundExplode>MortarBomb_Explode</soundExplode>
      <soundImpactAnticipate>MortarRound_PreImpact</soundImpactAnticipate>
      <soundAmbient>MortarRound_Ambient</soundAmbient>
    </projectile>
  </ThingDef>

