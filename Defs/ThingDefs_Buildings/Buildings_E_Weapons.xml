<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="BasedWeapon" ParentName="BasedMom" Abstract="True">
    <designationCategory>Rimatomics</designationCategory>
    <tickerType>Normal</tickerType>
    <castEdgeShadows>false</castEdgeShadows>
    <blockWind>true</blockWind>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>150</pathCost>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <constructEffect>ConstructMetal</constructEffect>
    <hasTooltip>true</hasTooltip>
    <staticSunShadowHeight>0</staticSunShadowHeight>
    <canOverlapZones>false</canOverlapZones>
  </ThingDef>

  <ThingDef ParentName="BasedWeapon">
    <defName>PPC</defName>
    <label>Pulsed Power Capacitor</label>
    <description>A PPC stores large amounts of energy, which can be discharged in a single pulse by energy weapons</description>
    <thingClass>Rimatomics.Building_PPC</thingClass>
    <graphicData>
      <texPath>Things/RimatomicsBuildings/PPC</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(4,6)</drawSize>
    </graphicData>
    <size>(2,4)</size>
    <fillPercent>0.60</fillPercent>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToBuild>800</WorkToBuild>
      <Mass>2000</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>-18</Beauty>
    </statBases>
    <building>
      <ignoreNeedsPower>true</ignoreNeedsPower>
      <ai_combatDangerous>true</ai_combatDangerous>
    </building>
    <costList>
      <Steel>400</Steel>
      <Component>8</Component>
    </costList>
    <comps>
      <li Class="CompProperties_Battery">
        <storedEnergyMax>6000</storedEnergyMax>
        <efficiency>0.9</efficiency>
        <startElectricalFires>false</startElectricalFires>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <designationHotkey>Misc4</designationHotkey>
    <rimatomicsPrerequisites>
      <li>EnergyWeapons6</li>
    </rimatomicsPrerequisites>
  </ThingDef>

  <ThingDef ParentName="BasedWeapon">
    <defName>PPCWeaponsConsole</defName>
    <label>Weapons Console</label>
    <description>A manned target acquisition computer required by all energy weapons</description>
    <size>(5,2)</size>
    <thingClass>Rimatomics.WeaponsConsole</thingClass>
    <graphicData>
      <texPath>Things/RimatomicsBuildings/WarRoom</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(7,4)</drawSize>
    </graphicData>
    <DrawGUIOverlay>True</DrawGUIOverlay>
    <fillPercent>1</fillPercent>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>9800</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <rotatable>true</rotatable>
    <costList>
      <Steel>250</Steel>
      <Component>16</Component>
    </costList>
    <comps>
      <li Class="CompProperties_Mannable">
        <manWorkType>Violent</manWorkType>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>175</basePowerConsumption>
        <startElectricalFires>true</startElectricalFires>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <building>
      <ai_combatDangerous>true</ai_combatDangerous>
    </building>
    <designationHotkey>Misc6</designationHotkey>
    <rimatomicsPrerequisites>
      <li>EnergyWeapons4</li>
    </rimatomicsPrerequisites>
  </ThingDef>

  <ThingDef ParentName="BasedWeapon">
    <defName>RadarDish</defName>
    <label>Target Acquisition System</label>
