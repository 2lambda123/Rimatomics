  <ThingDef ParentName="BasedWeapon">
    <defName>PPCRailgun</defName>
    <label>Railgun</label>
    <description>A weapon that uses a large electromagnetic pulse to launch a projectile to extreme ranges. Requires a weapons console and a PPC for power.</description>
    <thingClass>Rimatomics.Building_Railgun</thingClass>
    <graphicData>
      <texPath>Things/RimatomicsBuildings/RailgunBase</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,4)</drawSize>
      <damageData>
        <rect>(0.38,0.2,0.56375,0.8)</rect>
        <cornerTL>Damage/Corner</cornerTL>
      </damageData>
    </graphicData>
    <rotatable>true</rotatable>
    <size>(4,4)</size>
    <fillPercent>0.9</fillPercent>
    <comps>
      <li Class="CompProperties_Power">
